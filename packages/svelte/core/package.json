{
	"name": "@klnjs/svelte-core",
	"type": "module",
	"author": "Rune Klein (@rnkln)",
	"version": "1.0.0-beta.10",
	"license": "MIT",
	"homepage": "https://klnjs.github.io/basique",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/klnjs/basique.git",
		"directory": "packages/svelte/core"
	},
	"keywords": [
		"core",
		"svelte"
	],
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
	"exports": "./index.ts",
	"publishConfig": {
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"import": "./dist/index.js"
			}
		}
	},
	"scripts": {
		"lint": "eslint . --max-warnings 0",
		"clean": "basique clean",
		"check": "tsc --noEmit",
		"format": "prettier . --check",
		"build": "basique clean && basique build",
		"arrange": "basique arrange"
	},
	"devDependencies": {
		"@klnjs/basique": "workspace:*",
		"@klnjs/eslint-config": "4.0.5",
		"@klnjs/prettier-config": "4.0.5",
		"@klnjs/types": "workspace:*",
		"@klnjs/typescript-config": "4.0.5",
		"eslint": "9.11.1",
		"prettier": "3.3.3",
		"svelte": "4.2.19",
		"typescript": "5.6.2"
	}
}
