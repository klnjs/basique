{
	"name": "@klnjs/react-icons",
	"type": "module",
	"author": "Rune Klein (@rnkln)",
	"version": "1.0.0-beta.6",
	"license": "MIT",
	"homepage": "https://klnjs.github.io/basique",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/klnjs/basique.git",
		"directory": "packages/react/icons"
	},
	"keywords": [
		"a11y",
		"components",
		"glyphs",
		"icons",
		"pictograms",
		"react",
		"symbols"
	],
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
	"exports": "./index.ts",
	"publishConfig": {
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"import": "./dist/index.js"
			}
		}
	},
	"scripts": {
		"build": "basique build",
		"eslint": "eslint . --max-warnings 0",
		"prettier": "prettier . --check",
		"prepublish": "basique prepublish",
		"typescript": "tsc --noEmit",
		"postinstall": "bun ./scripts/create-icons.js"
	},
	"dependencies": {
		"@klnjs/react-core": "workspace:*",
		"@klnjs/icon": "workspace:*"
	},
	"devDependencies": {
		"@mdi/js": "7.4.47",
		"@klnjs/basique": "workspace:*",
		"@klnjs/eslint-config": "2.0.0",
		"@klnjs/prettier-config": "2.0.0",
		"@klnjs/typescript-config": "2.0.0",
		"@types/react": "18.3.3",
		"@types/react-dom": "18.3.0",
		"eslint": "8.57.0",
		"prettier": "3.3.3",
		"typescript": "5.5.3"
	},
	"peerDependencies": {
		"react": "^18.0",
		"react-dom": "^18.0"
	}
}
